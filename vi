#include <stdlib.h>

#include <time.h>

#include <stdio.h>


/**
 * main - Prints if number is postive, zero or negative
 * Return: Always (Success)
 *
 */
inr main(void)

{

       int n;
        
       srand(time(0));
       n = rand() - RAND_MAX / 2;

       if (n > 0)
       {
       printf("%d is zero\n", n);
       }
       else
       {
       printf("%d is negative\n" n);
       }
       return (0);

}
